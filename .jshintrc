{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "latedef": true,
    "immed": true,
    "indent": 4,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": false,
    "globalstrict": true,
    "maxdepth": 5,
    "maxstatements": 30,
    "maxcomplexity": 6,
    "latedef": "nofunc",
    "esnext": true,
    "expr": true,

    "browser": true,

    "predef": [
        //Node
        "require", "global", "console",

        //Test globals
        "describe", "it", "before", "after", "beforeEach", "beforeAll", "afterEach", "expect", "fixtures", "jQuery",

        //Sinon global
        "sinon", "stub", "spy"
    ]
}
